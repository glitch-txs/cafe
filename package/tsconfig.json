{
  "compilerOptions": {
    "target": "ES2016",
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  }
}